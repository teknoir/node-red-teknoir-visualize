{
  "name": "devstudio-debug",
  "version": "3.0.2",
  "description": "Low-code programming for event-driven applications",
  "homepage": "http://teknoir.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/teknoir/node-red-teknoir-visualize.git"
  },
  "main": "node_modules/node-red/red/red.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon --watch '../' $NODE_OPTIONS node_modules/node-red/red.js --userDir ./data",
    "start_": "node $NODE_OPTIONS node_modules/node-red/red.js --userDir ./data"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "node-red": "^3.0.2",
    "node-red-contrib-image-tools": "^2.0.4",
    "node-red-node-data-generator": "^1.0.1",
    "node-red-contrib-files": "github:tekn0ir/node-red-contrib-files#master",
    "node-red-contrib-webcamera": "github:tekn0ir/node-red-contrib-webcamera#master",
    "node-red-teknoir-visualize": "file:../",
    "nodemon": "^2.0.19"
  },
  "engines": {
    "node": ">=10"
  },
  "nodemonConfig": {
    "ext": "js,html"
  }
}
